{
  "name": "realestate-video-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "esbuild src/handlers/*/index.ts src/handlers/api/*.ts --bundle --platform=node --target=node20 --outdir=dist/handlers --format=esm --packages=external && echo '{\"type\":\"module\"}' > dist/package.json",
    "deploy": "npm run build && cd ../terraform && terraform apply -auto-approve"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.700.0",
    "@aws-sdk/client-dynamodb": "^3.700.0",
    "@aws-sdk/client-polly": "^3.700.0",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/client-sfn": "^3.700.0",
    "@aws-sdk/lib-storage": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0",
    "dotenv": "^16.4.5",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^22.9.0",
    "@types/uuid": "^10.0.0",
    "esbuild": "^0.24.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.0"
  }
}
